    "name": "Flask-Dev-Container",
    "image": "python:3.10.12",
    "dockerFile": "/app/DOCKERFILE",
    "extensions": ["ms-python.python"],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/sh"
    },
    "forwardPorts": [5000],
    "postCreateCommand": "/bin/sh -c 'python app/app.py'"
  }